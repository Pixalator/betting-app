      <div :class="{ opened: leftBarOpened }" class="nav__left">
        <div class="nav__left__item nav__left__toggle">
          <button @click="leftBarOpened = !leftBarOpened" class="btn btn--icon btn--no-bg"><span class="sr-only">Toggle navbar</span><svg viewBox="0 0 24 24"><path fill="currentColor" d="M19,13H3V11H19L15,7L16.4,5.6L22.8,12L16.4,18.4L15,17L19,13M3,6H13V8H3V6M13,16V18H3V16H13Z" /></svg></button>
